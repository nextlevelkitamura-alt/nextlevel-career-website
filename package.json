{
  "name": "nextlevel-career-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3003",
    "dev:host": "next dev -p 3003 -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:build": "docker build -t nextlevel-career-site .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.local nextlevel-career-site",
    "docker:test": "npm run docker:build && npm run docker:run",
    "deploy": "gcloud run deploy nextlevel-career-site --source . --region asia-northeast1 --allow-unauthenticated",
    "logs": "gcloud run services logs read nextlevel-career-site --region asia-northeast1 --limit 50"
  },
